$(function () {
  $('#t_cards-table').html('<%= j(render(:partial => 't_cards/t_card_table', :locals => { t_cards: @t_cards, visible_assignee: false, url: "mypage", status: @status, callback: "setTasksCount" })) %>')
  <% if @status == TCard.status.open %>
    $("#open-btn").addClass('mdl-color-text--primary');
    $("#close-btn").removeClass('mdl-color-text--primary');
  <% else %>
    $("#open-btn").removeClass('mdl-color-text--primary');
    $("#close-btn").addClass('mdl-color-text--primary');
  <% end %>
  <% # カード編集のモーダルイベントを設定する %>
  tableTdClickableAjax("t_cards-table-data", "edit_task_modal_event");
  <% # see. https://github.com/google/material-design-lite/wiki/How-the-Component-Handler-works %>
  componentHandler.upgradeAllRegistered();
});
