    <ul class="tool-bar-buttons">
      <li class="p-footerBar__labelSetting">
        <button class="mdl-button mdl-js-button kpter-color-text tool-bar-button"  data-type="keep">Keep</button>
      </li>
      <li>
        <button class="mdl-button mdl-js-button kpter-color-text tool-bar-button" data-type="problem">Problem</button>
      </li>
      <li>
        <button class="mdl-button mdl-js-button kpter-color-text tool-bar-button"  data-type="try">Try</button>
      </li>
    </ul>
  </div>
  <input type="hidden" id="board_id" name="board_id" value="<%= @board.id %>">
</div>
